<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/indexFragment">

    <fragment
        android:id="@+id/indexFragment"
        android:name="grg.app.open.ui.fragment.IndexFragment"
        android:label="fragment_blank"
        tools:layout="@layout/fragment_index" >
        <action
            android:id="@+id/action_indexFragment_to_squareFragment"
            app:destination="@id/squareFragment" />
    </fragment>
    <fragment
        android:id="@+id/squareFragment"
        android:name="grg.app.open.ui.fragment.SquareFragment"
        android:label="BlankFragment2" >
        <action
            android:id="@+id/action_squareFragment_to_indexFragment"
            app:destination="@id/indexFragment" />
    </fragment>
</navigation>